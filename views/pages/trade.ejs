<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="styles/reset.css">
    <link rel="stylesheet" href="styles/base.css">



    <script id="companyTemp" type="text/-template-mustache">
    <div id="{{compName}}" class="company">

        <img src="img/companies/{{compName}}.png" alt="">

        <h3>{{compName}}</h3>

        <h4>{{openP}}</h4>

        <h5>{{closeP}}</h5>

        {{{changeRatio}}}

        <div class ="chart"><canvas id="{{compName}}1" width="200" height="100"></canvas></div>

        <button type="button" id="{{compName}}BtnBuy" class="buy"> Trade </button>
        
     </div>
    </script>



</head>

<body>
    <main>
        <div id="header"></div>
        <div id="platFormContainer">

            <div id="tradePlatform">
                <div id="tradeHead">
                    <h1>Name</h1>
                    <h1>Open Price</h1>
                    <h1>Close Price</h1>
                    <h1>1M Change</h1>
                </div>
            </div>
        </div>

        <div id="sideBar">

        </div>
    </main>
    <section class="section">
        <div class="containter">
            <div id="logo"></div>
            <div id="profile"></div>
            <div id="arrow">
                <p></p>
                <div id="imgArrow"></div>
            </div>
            <div id="timer"></div>
            <div id="chartTrade"></div>

            <div id="formDiv">
                <form id="buyForm" action="">
                    <label id="balance" for="balance"></label><br>
                    <label for="Stocks" required>Stocks</label><br>
                    <input type="number"><br>
                    <label for="Price">Last Price</label><br>
                    <h1></h1>
                    <button id="btnS" type="button">SELL</button>
                    <button id="btnB" type="button">BUY</button><br>
                </form>
            </div>
        </div>
    </section>
    <footer></footer>
    <script src="https://code.jquery.com/jquery-3.6.0.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/mustache.js/4.1.0/mustache.js"
        integrity="sha512-SDpqS2G7Z70bGQSl26nhAdQj9YJoQqMOJVBVqE1EyEaAAzdvLFNZ0R06FzBu+E8xTcmbd2dBhvBJuJrbEUiKqA=="
        crossorigin="anonymous"></script>
    <script type="module" src="app1.js"></script>

</body>

</html>